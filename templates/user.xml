<templateSet group="user">
  <template name="for" value="for(int i = 0; i &lt; $VALUE$; i++) {&#10;&#9;$END$&#10;}&#10;" description="Create for loop" toReformat="true" toShortenFQNames="true">
    <variable name="VALUE" expression="suggestVariableName()" defaultValue="10" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="itma" value="for(java.util.Iterator $ITER$ = $MAP$.keySet().iterator(); $ITER$.hasNext(); ) {&#10;  $KEY_TYPE$ $KEY$ = ($KEY_TYPE$)$ITER$.next();&#10;  $ELEMENT_TYPE$ $VAR$ = ($ELEMENT_TYPE$)$MAP$.get($KEY$);&#10;  $END$&#10;}" description="Iterate elements of java.util.Map" toReformat="true" toShortenFQNames="true">
    <variable name="ITER" expression="suggestVariableName()" defaultValue="" alwaysStopAt="true" />
    <variable name="MAP" expression="variableOfType(&quot;java.util.Map&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="KEY_TYPE" expression="guessElementType(Container)" defaultValue="&quot;Object&quot;" alwaysStopAt="true" />
    <variable name="KEY" expression="suggestVariableName()" defaultValue="key" alwaysStopAt="true" />
    <variable name="ELEMENT_TYPE" expression="guessElementType(COLLECTION)" defaultValue="&quot;Object&quot;" alwaysStopAt="true" />
    <variable name="VAR" expression="suggestVariableName()" defaultValue="value" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="itpro" value="for(java.util.Iterator $ITER$ = $PROP$.keySet().iterator(); $ITER$.hasNext(); ) {&#10;  String $KEY$ = (String)$ITER$.next();&#10;  String $VALUE$ = $PROP$.getProperty($KEY$);&#10;  $END$&#10;}" description="Iterate elements of java.util.Properties" toReformat="true" toShortenFQNames="true">
    <variable name="ITER" expression="suggestVariableName()" defaultValue="" alwaysStopAt="true" />
    <variable name="PROP" expression="variableOfType(&quot;java.util.Properties&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="KEY" expression="suggestVariableName()" defaultValue="&quot;key&quot;" alwaysStopAt="true" />
    <variable name="VALUE" expression="suggestVariableName()" defaultValue="&quot;value&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
