<templateSet group="test">
  <template name="test" value="@Test&#10;public void $methodName$() throws Exception {&#10;  $END$&#10;}" description="Declare JUnit test" toReformat="false" toShortenFQNames="true">
    <variable name="methodName" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="before" value="private $CLASS_UNDER_TEST$ $TEST_INSTANCE$;&#10;&#10;@Before&#10;public void setUp() {&#10;    $TEST_INSTANCE$ = new $CLASS_UNDER_TEST$(); &#10;}&#10;&#10;$END$" description="@Before setup method" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_UNDER_TEST" expression="className()" defaultValue="" alwaysStopAt="true" />
    <variable name="TEST_INSTANCE" expression="suggestVariableName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>